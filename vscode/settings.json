{
  "window.autoDetectColorScheme": true,
  "workbench.colorTheme": "Default Light+",
  "security.workspace.trust.untrustedFiles": "open",
  "editor.suggestSelection": "first",
  "editor.fontSize": 14,
  "editor.accessibilitySupport": "off",
  "editor.cursorSurroundingLines": 5,
  "problems.showCurrentInStatus": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.profiles.windows": {
    "GitBash": {
      "path": "C:\\Program Files\\Git-2.37.2.2\\bin\\bash.exe",
      "args": ["--login"]
    }
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 2000,
  "files.exclude": {
    "coverage__/": true,
    "somefile__": true
  },
  "search.exclude": {
    "node_modules/": true,
    "somefile___": true
  },
  "git.autofetch": true,
  // "gitlens.mode.active": "zen",
  "terminal.integrated.defaultProfile.windows": "GitBash",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1,
    "asvetliakov.vscode-neovim": 1
  },
  "vim.vimrc.enable": true,
  "vim.statusBarColorControl": false,
  "vim.sneak": true,
  "vim.sneakReplacesF": true,
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.easymotion": true,
  "vim.smartRelativeLine": true,
  "vim.leader": "<space>",
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.highlightedyank.enable": true,
  "vim.useSystemClipboard": false,
  "vim.useCtrlKeys": true,
  "vim.handleKeys": {
    "<C-w>": false,
    "<C-a>": false,
    "<C-f>": true,
    "<C-x>": false,
    "<C-e>": false,
    "<C-c>": false,
    "<C-u>": true,
    "<C-v>": true
  },
  "vim.insertModeKeyBindings": [
    {"before": ["j", "k"], "after": ["<Esc>"]},
    {"before": ["k", "j"], "after": ["<Esc>"]}
  ],
  "vim.visualModeKeyBindings": [
    {"before": ["<leader>", "y"], "after": ["\"", "+", "y"]},
    {"before": ["<leader>", "p"], "after": ["\"", "+", "p"]},
    {"before": [">"], "commands": ["editor.action.indentLines"]},
    {"before": ["<"], "commands": ["editor.action.outdentLines"]},
    {"before": ["<leader>", "/"], "commands": ["editor.action.commentLine"]},
    {"before": ["<leader>", "z"], "commands": ["workbench.action.toggleZenMode"]},
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {"before": ["u"], "commands": ["undo"]},
    {"before": ["U"], "commands": ["redo"]},
    {"before": ["=", "="], "after": ["g", "g", "=", "G", "`", "`"]},
    {"before": ["<leader>", "y"], "after": ["\"", "+", "y"]},
    {"before": ["<leader>", "p"], "after": ["\"", "+", "p"]},
    {"before": ["g", "r"], "commands": ["editor.action.goToReferences"]},
    {"before": ["g", "o"], "commands": ["breadcrumbs.toggleToOn"]},
    {"before": ["<space>"], "commands": ["workbench.action.files.save"]},
    {
      "before": ["<leader>", "n"],
      "commands": ["workbench.files.action.showActiveFileInExplorer"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.terminal.focus"]
    },
    {
      "before": ["<leader>", "N"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {"before": ["<leader>", "e"], "commands": ["workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"]},
    {"before": ["J"], "commands": ["workbench.action.previousEditor"]},
    {"before": ["K"], "commands": ["workbench.action.nextEditor"]},
    {"before": ["[", "d"], "commands": ["editor.action.marker.next"]},
    {"before": ["]", "d"], "commands": ["editor.action.marker.prev"]},
    {"before": ["<leader>", "l"], "commands": ["eslint.executeAutofix"]},
    {"before": ["<leader>", "r", "n"], "commands": ["editor.action.rename"]},
    {"before": ["<leader>", "/"], "commands": ["editor.action.commentLine"]},
    {"before": ["<C-o>"], "commands": ["workbench.action.navigateBack"]},
    {"before": ["<C-i>"], "commands": ["workbench.action.navigateForward"]},
    {"before": ["<leader>", "z"], "commands": ["workbench.action.toggleZenMode"]},
    {"before": ["<leader>", "f"], "commands": ["workbench.action.findInFiles"]}
  ],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": {
    "editor.maxTokenizationLineLength": 2500
  },
  "[java]": {
    "editor.suggest.snippetsPreventQuickSuggestions": false
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "editor.inlineSuggest.enabled": true,
}