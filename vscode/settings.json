{
  "security.workspace.trust.untrustedFiles": "open",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "editor.fontSize": 12,
  "editor.accessibilitySupport": "off",
  "terminal.integrated.env.osx": {"FIG_NEW_SESSION": "1"},
  "vim.sneak": true,
  "vim.sneakReplacesF": true,
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.easymotion": true,
  "vim.vimrc.enable": true,
  "vim.leader": "<space>",
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.handleKeys": {"<C-a>": false, "<C-f>": false},
  "vim.insertModeKeyBindings": [
    {"before": ["j", "k"], "after": ["<Esc>"]},
    {"before": ["k", "j"], "after": ["<Esc>"]}
  ],
  "vim.visualModeKeyBindings": [
    {"before": ["j", "k"], "after": ["<Esc>"]},
    {"before": ["k", "j"], "after": ["<Esc>"]},
    {"before": [">"], "commands": ["editor.action.indentLines"]},
    {"before": ["<"], "commands": ["editor.action.outdentLines"]}
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {"before": ["u"], "commands": ["undo"]},
    {"before": ["U"], "commands": ["redo"]},
    {"before": ["=", "="], "after": ["gg=G"]},
    {"before": ["<space>"], "commands": ["workbench.action.files.save"]},
    {
      "before": ["<leader>", "n"],
      "commands": ["workbench.files.action.showActiveFileInExplorer"]
    },
    {
      "before": ["<leader>", "N"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.action.quickOpenLeastRecentlyUsedEditorInGroup"]
    },
    {"before": ["<leader>", "r", "n"], "commands": ["editor.action.rename"]},
    {"before": ["<leader>", "/"], "commands": ["editor.action.commentLine"]},
    {"before": ["<C-o>"], "commands": ["workbench.action.navigateBack"]},
    {"before": ["<C-i>"], "commands": ["workbench.action.navigateForward"]}
  ]
}